.card5 {
  flex-direction: row;
  background-color: #fff;
  padding: 20rpx;
}
.card5 > .img {
  content-visibility: auto;
  background-color: #e7f5ff;
  margin-right: 20rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 120rpx;
}
.card5 > .txtBox {
  flex: 1;
  justify-content: space-around;
}
.card5 > .txtBox > .content {
  color: #495057;
  font-size: 30rpx;
}
.card5 .box {
  flex-direction: row;
  justify-content: space-between;
}
.card5 .box > .head {
  font-weight: 600;
  font-size: 35rpx;
}
.card5 .box > .date {
  color: #868e96;
  font-size: 20rpx;
}

